{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "apiImageName": {
        "value": "${SERVICE_API_IMAGE_NAME=k8seteste2e.azurecr.io/samples/springboard-api:latest}"
      },
      "webImageName": {
        "value": "${SERVICE_WEB_IMAGE_NAME=k8seteste2e.azurecr.io/samples/springboard-web:latest}"
      },
      "resourceGroupName": {
        "value": "${AZURE_RESOURCE_GROUP}"
      },
      "tags": {
       "value": "${AZURE_TAGS}"
      }
    }
}
